<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:wxy="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.parrot.car.bitmaplinesmoothview.SmoothLineView
        android:id="@+id/sv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        wxy:imgDown="@drawable/strong_flow_nd"
        wxy:imgTop="@drawable/strong_flow_na"
        wxy:onStartLeft="true" />

    <Button
        android:id="@+id/ch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorPrimary"
        android:text="change"/>
</FrameLayout>